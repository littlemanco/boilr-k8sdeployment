{
  "Service": "Webserver",
  "Application": "Nginx",
  "Role": "Webserver",
  "Namespace": "www-example-com",
  "Replicas": 2,
  "MaxSurge": 1,
  "MaxUnavailable": 1,
  "Image": "nginx",
  "ImageVersion": "latest",
  "ApplicationPort": 80,
  "ApplicationPortProtocol": [
     "TCP",
     "UDP"
  ], 
  "ApplicationBootTime": 5,
  "ApplicationHealthCheckPeriod": 1
}
