{
  "Service": "Webserver",
  "Application": "Nginx",
  "Role": "Webserver",
  "Namespace": "www-example-com",
  "Replicas": 2,
  "MaxSurge": 1,
  "MaxUnavailable": 1,
  "Image": "nginx",
  "ImageVersion": "latest",
  "CpuLimit": "1",
  "MemoryLimit": "512",
  "CpuRequest": "1",
  "MemoryRequest": "512",
  "ApplicationPort": 80,
  "ApplicationPortProtocol": [
     "TCP",
     "UDP"
  ], 
  "ApplicationPortName": "http",
  "ProbeType": [
    "tcp",
    "http"
  ],
  "ApplicationBootTime": 5,
  "ApplicationHealthCheckPeriod": 1
}
